{
    "name": "huskycms/hc-backend",
    "type": "huskycms-backend",
    "description": "HuskyCMS backend",
    "authors": [
        {
            "name": "Mylotskyi Oleksii",
            "email": "spalaxinco@gmail.com"
        }
    ],
    "keywords": [
        "huskycms", "admin"
    ],
    "license": "MIT",
    "homepage": "http://huskycms.com/hc-backend",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.milsdev.com"
        }
    ],
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*"
    },
    "require": {
        "php": ">=5.3.3",
        "huskycms/hc-core": "dev-master",
        "spalax/zf2-libs": "dev-master",
        "bjyoungblood/bjy-authorize": "1.4.*",
        "spalax/zf2-file-uploader": "dev-master",
        "zendframework/zendframework": "2.3.0",
        "doctrine/doctrine-module": "0.*",
        "doctrine/doctrine-orm-module": "0.*",
        "zf-commons/zfc-user": "0.*",
        "zf-commons/zfc-user-doctrine-orm": "0.*",
        "doctrine/migrations": "v1.0-ALPHA1",
        "zendframework/zend-developer-tools": "0.*",
        "rwoverdijk/assetmanager": "1.3.5"
    },
    "autoload": {
        "psr-0": {
            "HcBackend": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "minimum-stability": "dev"
}
